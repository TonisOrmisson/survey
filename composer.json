{
    "name": "andmemasin/survey",

    "type": "library",
    "license":"proprietary",

    "description": "Cross-app Survey library",
    "keywords": ["yii2", "survey", "andmemasin"],
    "authors": [
        {
            "name": "TÃµnis Ormisson",
            "email": "tonis@andmemasin.eu"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],

    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1.2",
        "yiisoft/yii2": "^2.0.14",
        "andmemasin/yii2-myabstract": "^5.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8"
    },
    "config": {
        "platform": {
            "php": "8.1.2"
        },
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    },
    "autoload": {
        "psr-4": {
            "andmemasin\\survey\\": "src"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.7.x-dev"
        }
    }

}
